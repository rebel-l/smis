{
  "run": {
    "tests": true
  },
  "linters": {
    "enable": [
      "bodyclose",
      "deadcode",
      "dupl",
      "errcheck",
      "gochecknoinits",
      "goconst",
      "gocritic",
      "gocyclo",
      "golint",
      "gosec",
      "gosimple",
      "govet",
      "ineffassign",
      "interfacer",
      "lll",
      "maligned",
      "misspell",
      "nakedret",
      "prealloc",
      "staticcheck",
      "structcheck",
      "stylecheck",
      "typecheck",
      "unconvert",
      "unparam",
      "unused",
      "varcheck"
    ]
  }
}